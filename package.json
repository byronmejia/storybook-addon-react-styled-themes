{
    "name": "@byronmejia/storybook-addon-react-styled-themes",
    "version": "0.0.1",
    "description": "An add-on for storybook to allow changing themes.",
    "main": "dist/index.js",
    "repository": "git@github.com:byronmejia/storybook-addon-react-styled-themes.git",
    "author": "Byron Mejia <hello@byronis.me>",
    "license": "MIT",
    "private": false,
    "scripts": {
        "precommit": "lint-staged",
        "commit": "git-cz",
        "test": "jest",
        "test:ci": "jest --runInBand --ci --reporters=\"jest-junit\"",
        "format": "prettier --parser typescript --write --config ./.prettierrc **/*.{ts,tsx}",
        "format:check": "prettier --check --parser typescript --config ./.prettierrc **/*.{ts,tsx}",
        "storybook": "start-storybook -p 6006",
        "storybook:build": "build-storybook -o ./dist/storybook",
        "build": "rollup -c",
        "clean": "rimraf ./dist"
    },
    "devDependencies": {
        "@release-it/conventional-changelog": "^1.1.0",
        "@types/jest": "^24.9.0",
        "commitizen": "^4.0.3",
        "cz-conventional-changelog": "^3.0.2",
        "git-hooks": "^1.1.10",
        "jest": "24.9.0",
        "jest-junit": "^10.0.0",
        "lint-staged": "^10.0.8",
        "prettier": "^1.19.1",
        "react": "^16.8",
        "react-dom": "^16.8",
        "react-is": "^16.8",
        "react-test-renderer": "^16.12.0",
        "regenerator-runtime": "*",
        "release-it": "^12.4.3",
        "rimraf": "^3.0.0",
        "rollup": "^1.30.1",
        "rollup-plugin-typescript2": "^0.25.3",
        "styled-components": "^5.0.1",
        "typescript": "^3.7.5"
    },
    "dependencies": {
        "@storybook/addons": "^5.3.9",
        "@storybook/api": "^5.3.9",
        "@storybook/components": "^5.3.9",
        "@storybook/theming": "^5.3.9",
        "@types/styled-components": "^5.0.1",
        "tslib": "^1.10.0"
    },
    "peerDependencies": {
        "@storybook/addons": "^5.3.9",
        "@storybook/api": "^5.3.9",
        "@storybook/components": "^5.3.9",
        "@storybook/theming": "^5.3.9",
        "react": "^16.8",
        "react-dom": "^16.8"
    },
    "files": ["dist/**/*", "register.js"]
}
